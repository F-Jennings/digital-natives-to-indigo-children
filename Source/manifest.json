{
	"manifest_version": 2,

	"name": "Digital Natives to Indigo Children",
    "short_name": "Indigo Children",
	"description": "Replaces the text 'Digital Natives' with 'Indigo Children'.",
	"author": "Fred Jennings",
	"version": "0.1",

	"icons": {
		"16": "icon16.png",
		"32": "icon32.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},

	"content_scripts":
	[
		{
			"matches": ["*://*/*"],
			"js": ["content_script.js"],
			"run_at": "document_end"
		}
	]
}

